<div class="modal fade" id="myModal" role="dialog">
    <div class="modal-dialog">
  <!-- */***************************************************** -->

<div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
        </div>
        <div class="modal-body">
        </div>
        
<h4 class="modal-title"><center>enter details</center></h4>

<%= form_for :event, url:event_create_path do |f| %>
<center>
<%= f.label :title %> <br><%= f.text_field :title %><br>
<%= f.label :venue %> <br><%= f.text_field :venue %><br>
<%= f.label :date %> <br><%= f.text_field :date %><br>
<%= f.label :time %> <br><%= f.text_field :time %><br>
<%= f.label :description %> <br><%= f.text_field :description %><br>
 <%= f.label :invitees %><br><%= f.text_field :invitees %><br>
<%= f.label :status %> <br>
<%= f.text_field :status %><br>
<%= f.submit "Submit" %>
</center>
<% end %>
<div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
